
module TestGeneration (num) {
	import "example.bib"
	options = [
		prefix : "userprefix-" 
		sorting: author asc
		category: year desc
	]
	styles = [
		author {
			font-styles: bold;
			font-color: blue;
			font-type: "Arial";
		}	
		title {
			font-styles: italic;
			font-color: red;
		}
	]
}

run TestGeneration
