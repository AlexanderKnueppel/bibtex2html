
module TestGeneration (alphanum) {
	import "example.bib"
	options = [
		prefix : "userprefix-" 
		sorting: author asc
		category: year desc
	]
	styles = [
		author {
			font-styles: bold;
			font-color: blue;
			font-family: "Arial";
		}	
		title {
			font-styles: italic;
			font-family: "Verdana";
		}
	]
}

run TestGeneration
